def is_lucky_number(num):
    digits = list(str(num))
    while len(digits) > 1:
        digits = [int(digit) for i, digit in enumerate(digits) if (i + 1) % 2 != 0]
    return digits[0] == 7

num = int(input())
if is_lucky_number(num):
    print("Lucky Number")
else:
    print("Not a Lucky Number")
