def pentagonal_prism_perimeter(side1, side2, side3, side4, side5):
    return side1 + side2 + side3 + side4 + side5

base_perimeter = float(input("Enter the perimeter of the base pentagon: "))
base_area = float(input("Enter the area of the base pentagon: "))
height = float(input("Enter the height of the pentagonal prism: "))

print("Perimeter of the Base of Pentagonal Prism:", pentagonal_prism_perimeter(side1, side2, side3, side4, side5))
