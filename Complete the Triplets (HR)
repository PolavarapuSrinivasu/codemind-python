def compareTriplets(a, b):
    c=0
    s=0
    for i in range(0,len(a)):
        for j in range(0,len(b)):
            if i==j:
                if a[i]>b[i]:
                    c+=1
                elif a[i]<b[i]:
                    s+=1
                else:
                    continue
    return c,s    
